{
  "inputs": [
    {
      "id": "github_pat",
      "description": "GitHub Personal Access Token",
      "type": "promptString",
      "password": true
    }
  ],
  "servers": {
    "github": {
      "url": "https://api.githubcopilot.com/mcp/",
      "requestInit": {
        "headers": {
          "Authorization": "Bearer ${input:github_pat}"
        }
      }
    }
  },
}
